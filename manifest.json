{
  "manifest_version": 3,
  "name": "Form Filler",
  "version": "1.0",
  "description": "Auto-fill form fields using short keys",
  "permissions": ["storage", "activeTab","input"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
